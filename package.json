{
  "name": "react-solutions",
  "version": "0.0.12",
  "description": "React Solutions",
  "author": "Hamed Fathi",
  "license": "MIT",
  "repository": "https://github.com/HamedFathi/React-Solutions",
  "keywords": [
    "react",
    "hooks",
    "templates"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "dist/index.js",
  "module": "dist/react-solutions.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom"
  },
  "peerDependencies": {
    "react": ">=17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "dependencies": {
    "styl-injector": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "husky": "^7.0.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
