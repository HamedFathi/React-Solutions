{
  "name": "react-solutions",
  "version": "0.0.18",
  "description": "React Solutions",
  "author": "Hamed Fathi",
  "license": "MIT",
  "repository": "https://github.com/HamedFathi/React-Solutions",
  "keywords": [
    "react",
    "hooks",
    "templates",
    "ui",
    "components",
    "utilities"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "dependencies": {
    "styl-injector": "^1.4.0"
  },
  "peerDependencies": {
    "react": ">=17.0",
    "react-dom": ">=17.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.2",
    "@types/react-dom": "^17.0.9",
    "@types/react": "^17.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.5",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "build": "npm run build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "files": [
      "/lib"
    ]
  }
}
